/* Breadcrumbs */

#content {
  .breadcrumbs {
    margin-bottom: 20px;
    .breadcrumb-link {
      a {
        color: rgb(0, 107, 184);
        text-decoration: none;
        outline: none;
        outline-offset: 0;

        font-size: 14px;
        user-select: text;
        font-weight: 500;
        text-align: center;
        vertical-align: baseline;

        &:hover, &:focus {
          text-decoration: underline !important;
        }

        .home-link {
          content: url('data:image/svg+xml;utf8,<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" stroke="color: rgb(0, 107, 184);" viewBox="0 0 16 16" role="img" data-icon-type="home" data-is-loaded="true" aria-hidden="true"><path d="m8.13 1.229 5.5 4.47a1 1 0 0 1 .37.777V14a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1V6.476a1 1 0 0 1 .37-.776l5.5-4.471a1 1 0 0 1 1.26 0ZM13 6.476 7.5 2.005 2 6.475V14h11V6.476Z"></path></svg>');
          display: inline-block;
          width: 12px;
          height: 12px;
          vertical-align: middle;
          transform: translate(0px, -2px);
          stroke-width: 0.15px;
          color: rgb(0, 107, 184);
          stroke: rgb(0, 107, 184);
          flex-shrink: 0;
        }
      }
    }

  }

  #title-page-breadcrumb {
    border-bottom: none;
    padding-bottom: 0;
  }

  .chevron-right {
    padding-left: 3px;
    padding-right: 7px;
    font-size: 14px;
    font-weight: 300;
  }

  .navheader {
    border-bottom: 1px solid #ccc;
    padding-bottom: 5px;
    margin: 0;
    display: none;
    width: 100%;
  }

  .navfooter {
    border-top: 1px dotted #E8E8E8;
    padding-top: 10px;
    margin: 60px 0 15px;
  }
}

/* The classname is mispelled across the templates and on Elastic.co as well */
.breadcrum-wrapper a {
font-size: 18px !important;
}

/* Previous and next pages */

#content {
  .navheader, .navfooter {
    .prev, .next {
      font-size: 14px;
      @media screen and (max-width: 800px) {
        font-size: 13px;
      }
      @media screen and (max-width: 500px) {
        font-size: 12px;
      }
    }
    .prev {
      float: left;
      width: 50%;
    }
    .next {
      text-align: right;
      width: 50%;
      display: inline-block;
      float: right;
    }
  }
}

#otp {
  font-size: 1.1429rem;
  font-weight: 600;
}


