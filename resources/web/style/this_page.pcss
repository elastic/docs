#this_page {
  padding-bottom: 20px;
  display: block;
  border-bottom: 1px solid #dee2e6;
  max-height:40vh;
  overflow: auto;
  @media screen and (max-width: 992px) {
    display: none;
  }
  h2 {
    font-size: 1.5em;
    line-height: 1.5em;
  }
  ul {
    list-style: none;
    margin: 0;
    font-size: 0.85em;
    padding: 0;
  }
  li {
    line-height: 1.2em;
    margin-top: .3em;
    margin-bottom: .3em;
  }
  .active {
    font-weight: 700;
    font-style: ul;
  }
}
